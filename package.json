    "finalhandler": "^1.0.0",
    "lodash": "^4.17.2",
    "node-sass": "^4.5.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
